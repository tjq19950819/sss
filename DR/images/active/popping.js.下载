define(function(e,g,f){function d(){this.cfg={hasTitle:!1,title:"\u6e29\u99a8\u63d0\u793a",hascontent:!0,content:"\u64cd\u4f5c\u6210\u529f\uff01",hasBtns:!1,btns:{},hideTime:3E3,result:!1,hasintegral:!1,otherintegral:!1,Addit:"+",callback:null}}e("zepto");d.prototype={init:function(b){var a=$.extend(this.cfg,b);b='<div class="popping" id="popObj"><div class="popMain fix" id="popMain">';a.hasTitle&&(b+="<h5>"+a.title+"</h5>");a.hascontent&&(b+='<div class="popContent">'+a.content+"</div>");a.hasintegral?
b+='<p class="integralnum"><span>'+a.Addit+"</span><i>"+a.Num+"</i>"+a.Numword+"</p>":a.otherintegral&&(b+='<p class="integralnum integralnum_other"><span class="otherntegral">'+a.Addit+"</span><i>"+a.Num+"</i>"+a.Numword+"</p>");a.hasBtns&&(b+='<div class="popBtns fix">',a.btns.cancel&&a.btns.confirm?(b+='<a class="popBtn-ok" id="popBtnOk" href="javascript:;">'+a.btns.confirm+"</a>",b+='<a class="popBtn-qx" id="popBtnQx" href="javascript:;">'+a.btns.cancel+"</a>"):a.btns.confirm?b+='<a class="popBtn-ok" id="popBtnOk" href="javascript:;" style="float:inherit;">'+
a.btns.confirm+"</a>":a.btns.cancel&&(b+='<a class="popBtn-qx" id="popBtnQx" href="javascript:;" style="float:inherit;">'+a.btns.cancel+"</a>"),b+="</div>");b+="</div></div>";$("body").append(b);var c=$("#popObj");$("#popMain");c.off("touchmove").on("touchmove",function(a){a.stopPropagation();a.preventDefault();return!1}).off("click").on("click",function(a){c.remove();return!1});a.hideTime&&setTimeout(function(){c.remove();a.callback&&a.callback()},a.hideTime);a.callback&&($("#popBtnOk").off("click").on("click",
function(){a.result=!0;a.callback(a.result);c.remove();return!1}),$("#popBtnQx").off("click").on("click",function(){a.result=!1;a.callback(a.result);c.remove();return!1}))}};f.exports=d});